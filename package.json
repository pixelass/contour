{
  "name": "contour",
  "version": "0.0.0",
  "private": true,
  "description": "A lightweight but very advanced grid-system library.",
  "repository": "git@github.com:pixelass/contour.git",
  "license": "MIT",
  "author": "Gregor Adams <greg@pixelass.com>",
  "main": "n.a.",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "npx lerna bootstrap",
    "prebuild": "npm run setup -- --build && npx lerna run clean",
    "build": "npx lerna run build",
    "build-storybook": "npx cross-env@7.0.3 NODE_ENV=production build-storybook -c .storybook -o public --quiet",
    "build-storybook:local": "build-storybook -c .storybook -o public --quiet",
    "clean": "npx lerna run clean",
    "predev": "npm run clean; npm run setup",
    "dev": "start-storybook",
    "eslint": "npx eslint './packages/**/*.{ts,tsx}'",
    "lint": "npm run tsc:noEmit; npm run eslint",
    "ncu:major": "npx npm-check-updates@11.8.3",
    "ncu:minor": "npx npm-check-updates@11.8.3 -t minor",
    "ncu:patch": "npx npm-check-updates@11.8.3 -t patch",
    "prepare": "npx husky install && npm run setup",
    "prepublishOnly": "npm run build",
    "prerelease": "npm run build",
    "release": "lerna publish --conventional-commits",
    "prerelease:beta": "npm run build",
    "release:beta": "lerna publish --canary --preid beta --pre-dist-tag beta",
    "prerelease:rc": "npm run build",
    "release:rc": "npx lerna publish --canary --preid rc --pre-dist-tag next",
    "serve:storybook": "npx serve@12.0.0 ./public",
    "setup": "babel-node prepare/init.ts -x '.ts'",
    "spj": "npx sort-package-json@1.48 package.json './packages/*/*/package.json'",
    "tsc:noEmit": "tsc --noEmit",
    "unimported": "npx unimported@1.13.0",
    "watch": "npx lerna run watch"
  },
  "dependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/node": "^7.16.8",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@commitlint/config-lerna-scopes": "^16.2.2",
    "@emotion/babel-plugin": "^11.7.2",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@emotion/cache": "^11.7.1",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.8.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.4.4",
    "@mui/material": "^5.4.4",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-typescript": "^8.3.1",
    "@storybook/addon-docs": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/theming": "^6.4.19",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-xo-react": "^0.26.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "lerna": "^4.0.0",
    "prettier": "2.5.1",
    "react": "^17.0.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.69.2",
    "ts-node": "^10.7.0",
    "type-fest": "^2.12.0",
    "typescript": "^4.5.5",
    "uuid": "^8.3.2",
    "webpack": "^5.70.0"
  }
}
